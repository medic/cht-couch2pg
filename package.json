{
  "name": "medic-couch2pg",
  "version": "1.2.0",
  "main": "index.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/medic/medic-couch2pg.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "watch": "jest --watchAll",
    "ci": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "curl-request": "1.1.1",
    "grunt": "^0.4.0",
    "grunt-cli": "^1.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-mocha-test": "^0.12.0",
    "jest": "22.4.3",
    "jest-runner-mocha": "0.6.0",
    "knex": "0.14.6",
    "loglevel": "1.6.1",
    "loglevel-message-prefix": "3.0.0",
    "mocha": "5.1.1",
    "pg": "7.4.1",
    "pg-format": "1.0.4",
    "pg-promise": "8.4.0",
    "postgrator": "3.5.0",
    "pouchdb": "6.4.3",
    "rsvp": "4.8.2",
    "sinon": "4.5.0",
    "underscore": "1.9.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "pgutils"
    ],
    "collectCoverageFrom": [
      "<rootDir>/libs/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "globalSetup": "<rootDir>/setup.js"
  }
}
